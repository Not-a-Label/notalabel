# Production Server Configuration
# Copy this to .env.production-server and fill in your actual values

# Production Server Details
PRODUCTION_SERVER_IP=your.production.server.ip
PRODUCTION_SERVER_USER=root
PRODUCTION_SSH_KEY=/path/to/your/ssh/key

# Domain Configuration  
DOMAIN=not-a-label.art
API_DOMAIN=api.not-a-label.art

# GitHub Repository
GITHUB_REPO=https://github.com/Not-a-Label/notalabel.git

# Email for SSL certificates
SSL_EMAIL=admin@not-a-label.art

# Database Credentials (should match .env.secrets)
POSTGRES_PASSWORD=your_secure_db_password
DB_PASSWORD=your_secure_db_password

# JWT Secrets (should match .env.secrets)
JWT_SECRET=your_jwt_secret_key
SESSION_SECRET=your_session_secret_key
NEXTAUTH_SECRET=your_nextauth_secret_key

# API Keys (replace with real production keys)
OPENAI_API_KEY=sk-your-real-openai-api-key
SPOTIFY_CLIENT_ID=your-spotify-client-id
SPOTIFY_CLIENT_SECRET=your-spotify-client-secret
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Stripe Keys (replace with live keys)
STRIPE_SECRET_KEY=sk_live_your-real-stripe-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-real-webhook-secret
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_live_your-real-publishable-key

# Email Configuration
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=noreply@not-a-label.art
EMAIL_PASS=your-email-app-password

# Analytics
GOOGLE_ANALYTICS_ID=G-YOUR-REAL-GA-ID
MIXPANEL_TOKEN=your-real-mixpanel-token